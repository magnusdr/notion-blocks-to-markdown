    "test": "jest",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.11",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "esbuild": "^0.19.2",
    "eslint": "^8.47.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4"
  }
}
