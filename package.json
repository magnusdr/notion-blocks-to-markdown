{
  "name": "notion-blocks-to-markdown",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:bundle": "esbuild src/index.ts --bundle --outdir=dist --sourcemap",
    "build:types": "tsc --emitDeclarationOnly",
